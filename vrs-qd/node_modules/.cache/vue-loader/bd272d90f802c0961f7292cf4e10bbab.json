.redirect\r\n\t\t\t\t\t\t\tthis.$router.replace('/home')\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t})\r\n\t\t\t\t})\r\n\t\t\t},\r\n            //重置\r\n\t\t\tresetLoginForm() {\r\n\t\t\t\tconsole.log(this.$refs)\r\n\t\t\t\tthis.$refs.loginFormRef.resetFields()\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\tbody {}\r\n\r\n\t.app {\r\n\t\twidth: 500px;\r\n\t\theight: 350px;\r\n\t\tposition: absolute;\r\n\t\tleft: 50%;\r\n\t\ttop: 50%;\r\n\t\tmargin-left: -250px;\r\n\t\tmargin-top: -175px;\r\n\t\tborder: 1px solid gray;\r\n\t\tborder-radius: 30px;\r\n\r\n\t\tbox-shadow: 10px 10px 10px 10px #888888;\r\n\t}\r\n\r\n\t.demo-ruleForm {\r\n\t\twidth: 400px;\r\n\t\tmargin: 0 auto;\r\n\t\tmargin-top: 40px;\r\n\t}\r\n\r\n\t.btn {\r\n\t\tmargin-left: 135px;\r\n\t}\r\n</style>\n"]}]}